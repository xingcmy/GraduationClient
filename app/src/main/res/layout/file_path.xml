<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <com.cn.graduationclient.xingcmyAdapter.HoldTitle
        android:id="@+id/file_path_hold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="文件路径"
        app:type="10"
        app:text="@string/btn_send"/>

    <TextView
        android:id="@+id/text_file_path"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textColor="#000000"
        android:background="@drawable/login_user_password"/>
    <ImageView
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        android:id="@+id/file_path_img"/>
</LinearLayout>
