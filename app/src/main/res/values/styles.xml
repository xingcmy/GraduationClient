<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:textColorPrimary">@color/xingcmy.theme.dark_blue.textColorPrimary</item>
        <item name="android:textColorSecondary">@color/xingcmy.theme.dark_blue.textColorSecondary</item>

        <item name="android:windowBackground">@color/xingcmy.theme.dark_blue.background</item>

        <item name="colorControlNormal">?attr/colorAccent</item>
        <item name="colorControlActivated">@color/xingcmy.theme.dark_blue.colorControlActivated</item>
        <item name="colorControlHighlight">@color/xingcmy.theme.dark_blue.colorControlHighlight</item>

        <item name="android:statusBarColor" tools:targetApi="lollipop">@color/transparent</item>
    </style>



</resources>